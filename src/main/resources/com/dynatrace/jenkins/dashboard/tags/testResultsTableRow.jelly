<j:jelly xmlns:j="jelly:core">
	<j:choose>
		<j:when test="${it != null}">
			<td>${it.getValue()}</td>
			<td>${it.getExpectedMin()}</td>
			<td>${it.getExpectedMax()}</td>
			<td>${it.getViolationPercentage()}</td>
		</j:when>
		<j:otherwise>
			<td>-</td>
			<td>-</td>
			<td>-</td>
			<td>-</td>
		</j:otherwise>
	</j:choose>
</j:jelly>
